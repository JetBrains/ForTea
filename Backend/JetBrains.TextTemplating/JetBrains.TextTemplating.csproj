<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <DefineConstants>$(DefineConstants);JET_MODE_ASSERT</DefineConstants>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <OutputPath>..\output\JetBrains.TextTemplating\$(Configuration)</OutputPath>
    <LangVersion>latest</LangVersion>
    <RootNamespace>Microsoft.VisualStudio.TextTemplating</RootNamespace>
  </PropertyGroup>
  <Import Project="$(SolutionDir)targets\EnvDTETargets.targets"/>
  <ItemGroup>
    <PackageReference Include="JetBrains.EnvDTE" Version="$(EnvDTEVersion)"/>
    <PackageReference Include="JetBrains.EnvDTE80" Version="$(EnvDTEVersion)"/>
    <PackageReference Include="JetBrains.EnvDTE90" Version="$(EnvDTEVersion)"/>
    <PackageReference Include="JetBrains.EnvDTE90a" Version="$(EnvDTEVersion)"/>
    <PackageReference Include="JetBrains.EnvDTE100" Version="$(EnvDTEVersion)"/>
    <PackageReference Include="JetBrains.EnvDTE.Client" Version="$(EnvDTEVersion)"/>
  </ItemGroup>
</Project>